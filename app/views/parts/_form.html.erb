<%= form_for(@part) do |f| %>
    <% if @part.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@part.errors.count, "error") %> prohibited this part from being saved:</h2>

          <ul>
            <% @part.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="field">
      <%= f.label :part_id %><br>
      <%= f.text_field :part_id %>
    </div>
    <div class="field">
      <%= f.label :rate %><br>
      <%= f.number_field :rate %>
    </div>
    <div class="field">
      <%= f.label :std_people %><br>
      <%= f.number_field :std_people %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
